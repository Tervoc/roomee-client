\n\t\n\t\tsetValidated(true);\n\t}\n\n\treturn (\n\t\t<div className=\"component-wrapper\">\n\t\t\t<div className=\"form-holder\">\n\t\t\t\t<Image src=\"resources/images/roomee.png\" fluid/>\n\t\t\t\t<h1>roomee</h1>\n\t\t\t\t<p><b>New Account</b></p>\n\t\t\t\t<p>Already have an account? <Link to={'/login'}>Log in</Link></p>\n\n\t\t\t\t<Form noValidate validated={validated} onSubmit={handleSubmit}>\n\t\t\t\t\t<Form.Row>\n\t\t\t\t\t\t<Form.Group as={Col}>\n\t\t\t\t\t\t\t<Form.Label>First Name</Form.Label>\n\t\t\t\t\t\t\t<Form.Control\n\t\t\t\t\t\t\t\ttype=\"text\"\n\t\t\t\t\t\t\t\tplaceholder={'Matt'}\n\t\t\t\t\t\t\t\tvalue={firstNameFieldValue}\n\t\t\t\t\t\t\t\tonChange={handleFirstNameChange}\n\t\t\t\t\t\t\t\tmaxLength={'50'}\n\t\t\t\t\t\t\t\trequired\n\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t\t<Form.Control.Feedback type=\"invalid\">Field cannot be empty.</Form.Control.Feedback>\n\t\t\t\t\t\t</Form.Group>\n\t\t\t\t\t\t<Form.Group as={Col}>\n\t\t\t\t\t\t\t<Form.Label>Last Name</Form.Label>\n\t\t\t\t\t\t\t<Form.Control\n\t\t\t\t\t\t\t\ttype=\"text\"\n\t\t\t\t\t\t\t\tplaceholder={'Padgett'}\n\t\t\t\t\t\t\t\tvalue={lastNameFieldValue}\n\t\t\t\t\t\t\t\tonChange={handleLastNameChange}\n\t\t\t\t\t\t\t\tmaxLength={'50'}\n\t\t\t\t\t\t\t\trequired\n\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t\t<Form.Control.Feedback type=\"invalid\">You must provide a valid last name.</Form.Control.Feedback>\n\t\t\t\t\t\t</Form.Group>\n\t\t\t\t\t\t<Form.Group as={Col}>\n\t\t\t\t\t\t\t<Form.Label>Preferred Name</Form.Label>\n\t\t\t\t\t\t\t<Form.Control\n\t\t\t\t\t\t\t\ttype=\"text\"\n\t\t\t\t\t\t\t\tplaceholder={'MattyP'}\n\t\t\t\t\t\t\t\tvalue={preferredNameFieldValue}\n\t\t\t\t\t\t\t\tonChange={handlePreferredNameChange}\n\t\t\t\t\t\t\t\tmaxLength={'50'}\n\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t</Form.Group>\n\t\t\t\t\t</Form.Row>\n\t\t\t\t\t<Form.Row>\n\t\t\t\t\t\t<Form.Group as={Col}>\n\t\t\t\t\t\t\t<Form.Label>Email Address</Form.Label>\n\t\t\t\t\t\t\t<Form.Group>\n\t\t\t\t\t\t\t\t<Form.Control\n\t\t\t\t\t\t\t\t\ttype=\"t